<apex:page standardController="Pricing_Condition__c"
           extensions="PricingConditionUpdateController"
           action="{!init}" id="thePage" sidebar="false"
           tabStyle="Mass_Price_Increase__tab">

    <apex:includeScript value="{!$Resource.PendingApprovalFunctions}" />
    <script language="javascript">
        window.onload = function() {focus('{!$Component.thePage}');}
        function focus(id) {document.getElementById(id).focus();}
        function getConfirm(q) {a = confirm(q); return (a != 0);}
        function validateForm(f) {
            var msg = '';
            if (!paf_checkForAtleastOneRowSelection(f, 'rowcb')) {
                msg += '*** None selected to Update ***\n\n';
            }
            if (msg != '') {alert(msg); return false;}
            return true;
        }
    </script>

    <apex:variable var="listSize" value="15"/>
    <apex:variable var="decimals" value="4"/>
    <apex:variable var="locale" value="{!userLocale}"/>
    <apex:form id="theForm">
    <apex:pageMessages />

    <apex:tabPanel activeTabClass="activeTab"
                   tabClass="tabStyle"
                   headerSpacing="4"
                   switchType="client"
                   value="{!activeTab}"
                   rendered="{!pageBlockNumber = 1}">

        <apex:tab labelWidth="110px" label="Price Change Details" name="tabDChan">
            <apex:pageBlock title="Price Change Details"><apex:actionStatus id="tab1stat">
                <apex:facet name="start"><apex:outputPanel layout="block" styleClass="tab1div">
                    <apex:outputText styleClass="bold" value=" Please wait ...">
                        <apex:image value="{!$Resource.spinner}"/>
                    </apex:outputText></apex:outputPanel>
                </apex:facet>
                <apex:facet name="stop"><apex:outputPanel >
                <apex:panelGrid columns="2" cellpadding="5px" id="pcopts"
                                columnClasses="right,left">
                    <apex:panelGroup rendered="{!renderAccOpts}">
                        <apex:outputLabel value="Distribution Channel : "
                                          styleClass="bold"/>
                    </apex:panelGroup>
                    <apex:panelGroup rendered="{!renderAccOpts}">
                        <apex:selectRadio value="{!dchanCode}"
                                          layout="lineDirection"
                                          required="true">
                            <apex:selectOption itemValue="10" itemEscaped="false"
                                               itemLabel="10 - AWT &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" />
                            <apex:selectOption itemValue="50"  itemEscaped="false"
                                               itemLabel="50 - ASI &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"/>
                            <apex:selectOption itemValue="20"  itemEscaped="false"
                                               itemLabel="20 - APM &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"/>
                                <apex:actionSupport event="onclick"
                                                    action="{!changeDchan}"
                                                    rerender="theForm"
                                                    status="tab1stat" />
                        </apex:selectRadio>
                    </apex:panelGroup>

                    <apex:panelGroup >
                        <apex:outputLabel styleClass="bold"
                                          value="Price Increase By : "/>
                    </apex:panelGroup>
                    <apex:panelGroup >
                        &nbsp;&nbsp;Amount :&nbsp;
                        <apex:inputText size="8" style="font-size: 100%;"
                                        value="{!strPriceIncreaseAmount}"/>&nbsp;&nbsp;or
                        &nbsp;&nbsp;Percent :&nbsp;
                        <apex:inputText size="5" style="font-size: 100%;"
                                        value="{!strPriceIncreasePercnt}"/>&nbsp;&nbsp;
                    </apex:panelGroup>

                    <apex:panelGroup >&nbsp;</apex:panelGroup>
                    <apex:panelGroup >&nbsp;
                        Enter Zero to Extend Current Prices up to Effect-To-Date or for
                        {!maxDaysToExtend}&nbsp;More Days
                    </apex:panelGroup>

                    <apex:panelGroup >
                        <apex:outputLabel styleClass="bold"
                                          value="Price Increase Effect Date : "/>
                    </apex:panelGroup>
                    <apex:panelGroup >&nbsp;&nbsp;From :&nbsp;
                        <apex:inputField style="font-size: 100%;"
                                         value="{!pc.Valid_From_Date__c}"/>
                        &nbsp;&nbsp;To :&nbsp;
                        <apex:inputField style="font-size: 100%;"
                                         value="{!pc.Valid_To_Date__c}"/>&nbsp;&nbsp;
                    </apex:panelGroup>

                    <apex:panelGroup >&nbsp;</apex:panelGroup>
                    <apex:panelGroup >&nbsp;
                        From Date is used for a Change, and To Date is used on an Extend
                    </apex:panelGroup>

                    <apex:panelGroup >
                        <apex:outputLabel for="submitForApproval" styleClass="bold"
                                         value="Submit Prices Increases for Approval? "/>
                    </apex:panelGroup>
                    <apex:panelGroup >&nbsp;
                        <apex:inputCheckbox id="submitForApproval" style="font-size: 100%;"
                                         value="{!submitForApproval}"/>
                    </apex:panelGroup>

                    <apex:panelGroup >
                        <apex:outputLabel value="Saved Searches : "
                                          styleClass="bold"/>
                    </apex:panelGroup>
                    <apex:panelGroup >
                        <apex:selectList value="{!searchName}"
                                         size="1" styleClass="slist">
                            <apex:selectOptions value="{!searchNames}"/>
                        </apex:selectList>
                    </apex:panelGroup>

                    <apex:panelGroup >&nbsp;</apex:panelGroup>
                    <apex:panelGroup >
                        <apex:outputPanel >

                        <apex:commandButton action="{!getSavedSearchFlds}"
                                            value=" Get Saved Search "
                                            rerender="theForm" status="tab1stat"/>&nbsp;

                        <apex:commandButton action="{!getPricingConditions1}"
                                            value=" Get Pricing Conditions » "
                                            rerender="theForm" status="tab1stat"/>&nbsp;

                        <apex:commandButton rendered="{!renderAccOpts}"
                                            action="{!getSapSalesOrgs}"
                                            value=" Get Sales Orgs » "
                                            rerender="theForm" status="tab1stat"/>&nbsp;

                        <apex:commandButton rendered="{!!renderAccOpts}"
                                            action="{!getProductFamilies}"
                                            value=" Get Product Families (PH5) » "
                                            rerender="theForm" status="tab1stat"/>&nbsp;
                        </apex:outputPanel>
                    </apex:panelGroup>
                </apex:panelGrid></apex:outputPanel></apex:facet></apex:actionStatus>
            </apex:pageBlock>
        </apex:tab>

