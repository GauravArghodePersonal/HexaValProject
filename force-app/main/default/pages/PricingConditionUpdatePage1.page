<!-- -------------------------------------------------------------------------------------------------------------------- -->
<!-- ------------------------------------------------ Pricing Conditions ------------------------------------------------ -->
<!-- -------------------------------------------------------------------------------------------------------------------- -->

        <apex:pageBlock id="pclist" title="Pricing Conditions"
                        rendered="{!pageBlockNumber = 2}">
            <table width="100%">
<!-- -->
                <tr><td align="center">
                <apex:actionStatus id="saveSrchStat">
                    <apex:facet name="start">
                        <apex:outputText escape="false" styleClass="centerBtns"
                                         value=" Please wait ... ">
                            <apex:image value="{!$Resource.spinner}"/>
                        </apex:outputText>
                    </apex:facet>
                    <apex:facet name="stop">
                        <apex:outputPanel >
                        Update Saved Search or Save as a New Search&nbsp;:&nbsp;
                        <apex:inputText value="{!newSearch}" size="40"/>&nbsp;
                        <apex:commandButton action="{!saveSearchCriteria}"
                                            value=" Save Search Criteria "
                                            rerender="theForm" status="saveSrchStat"/>&nbsp;
                        <b>Search Name : </b>{!searchName}
                        </apex:outputPanel>
                    </apex:facet>
                </apex:actionStatus></td></tr>
                <tr><td>&nbsp;</td></tr>
<!-- -->
                <tr><td align="center">
                <apex:outputPanel >
                    <apex:commandButton action="{!gotoPageBlock1}"
                                        value=" « Back to Search "/>&nbsp;
                    <apex:outputPanel >
                        <apex:outputText value="Price Increase By - Amount : {!priceIncreaseAmount} "/>
                        <apex:outputText value=" or Percent : {!priceIncreasePercnt} %"/>
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        <apex:outputText value="Effect Date : From : "/>
                        <apex:outputField value="{!pc.Valid_From_Date__c}"/>&nbsp;To :&nbsp;
                        <apex:outputField value="{!pc.Valid_To_Date__c}"/>
                    </apex:outputPanel>&nbsp;
                                    <!--action="{!cloneAndSubmitForApproval}"-->
                                    <!--action="{!submitBatchJob}"-->
                    <apex:commandButton action="{!submitBatchJob}" disabled="{!disableUpdateBtn}"
                                        value=" Update / Extend Prices » "
                                        onclick="return getConfirm('Update Prices?');"/>
                </apex:outputPanel><br/><br/>
                (From Date is used for a Change, and To Date is used on an Extend)
            </td></tr></table><br/>
            <apex:pageBlockTable value="{!pclist}" var="p"
                                 rowClasses="row1,row2"
                                 style="font-size: 91%;" styleClass="pclist">
                <apex:column headerClass="right" styleClass="right" width="20px">
                    <apex:facet name="header">#</apex:facet>
                    <apex:outputText value="{!p.recordNumber}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Pricing<br/>Condition</apex:facet>
                    <apex:outputLink value="/{!p.pricingCondition.Id}">
                        <apex:outputField value="{!p.pricingCondition.Name}"/>
                    </apex:outputLink>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Material Name<br/>Number - BaseCode</apex:facet>
                    <apex:outputLink value="/{!p.pricingCondition.Material2__r.Id}">
                        <apex:outputText value="{!p.pricingCondition.Material2__r.Material_Desc__c}"/>
                    </apex:outputLink>
                    <br/><apex:outputText value="{!p.materialNumber} - {!p.pricingCondition.Material2__r.Material_Base_Code_Desc__c}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Account Name<br/>and Number</apex:facet>
                    <apex:outputLink value="/{!p.pricingCondition.Account__r.Id}">
                        <apex:outputText value="{!p.pricingCondition.Account__r.Name}"/>
                    </apex:outputLink>
                    <br/><apex:outputText value="{!p.accountNumber} - {!p.pricingCondition.Account_Level_Condition_Applies_To__c}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Sales Office<br/>and Group</apex:facet>
                    <apex:outputText value="{!p.salesOffice}"/><br/>
                    <apex:outputText value="{!p.salesgroup}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Product Family (PH5)<br/>and Sales Org</apex:facet>
                    <apex:outputText value="{!p.ph5}"/><br/>
                    <apex:outputText value="{!p.pricingCondition.Material2__r.Sales_Org_Code__c}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Product Segment (PH6)<br/>and SubSegment (PH7)</apex:facet>
                    <apex:outputText value="{!p.ph6}"/><br/>
                    <apex:outputText value="{!p.ph7}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Current Price<br/>New Price</apex:facet>
                    <apex:outputText >
                        <c:Currency value="{!p.pricingCondition.Base_Price__c}"
                                    code="{!p.pricingCondition.CurrencyIsoCode}"
                                       decimals="{!decimals}" locale="{!locale}"/>
                    </apex:outputText><br/>
                    <apex:outputText >
                        <c:Currency value="{!p.basePrice}"
                                    code="{!p.pricingCondition.CurrencyIsoCode}"
                                       decimals="{!decimals}" locale="{!locale}"/>
                    </apex:outputText>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Valid<br/>From To</apex:facet>
                    <apex:outputField value="{!p.pricingCondition.Valid_From_Date__c}"/><br/>
                    <apex:outputField value="{!p.pricingCondition.Valid_To_Date__c}"/>
                </apex:column>
                <apex:column breakBefore="true" colspan="99"
                             styleClass="right">
                    <apex:outputText style="font-weight: bold; color: red;" value="{!p.comments}"/>
                </apex:column>
            </apex:pageBlockTable>
            <apex:pageBlockButtons location="top">
                <apex:panelGrid bgcolor="#f8f8f8" columns="2" width="100%">
                    <apex:outputText value="Records: {!pc1.recordFrom}-{!pc1.recordTo} of {!pc1.resultSize}"
                                     styleClass="bold nowrap"/>
                    <apex:outputText value="Page: {!pc1.pageNumber} of {!pc1.lastPageNumber}"
                                     styleClass="bold nowrap"/>
                </apex:panelGrid>
            </apex:pageBlockButtons>
            <apex:facet name="footer">
                <apex:outputPanel layout="block">
                <table class="bold" style="font-size: 91%;">

                    <tr><td class="center" width="15%">
                        <apex:outputText value="Records: {!pc1.recordFrom} - {!pc1.recordTo} of {!pc1.resultSize}"
                                         styleClass="nowrap"/>
                    </td>

                    <td class="center" width="70%">
                            <apex:actionStatus id="pclistPC">
                                <apex:facet name="start">
                                    <apex:outputText styleClass="bold" value=" Please wait ...">
                                        <apex:image value="{!$Resource.spinner}"/>
                                    </apex:outputText>
                                </apex:facet>
                                <apex:facet name="stop">
                                <apex:outputPanel >
                                <apex:outputText value="Rows Per Page:"/>&nbsp;
                                <apex:selectList value="{!pc1.pageSize}" size="1" title="Rows Per Page:">
                                    <apex:actionSupport event="onchange" action="{!pc1Size}"
                                                    rerender="pclist" status="pclistPC"/>
                                    <apex:selectOption itemLabel="10" itemValue="10"/>
                                    <apex:selectOption itemLabel="20" itemValue="20"/>
                                    <apex:selectOption itemLabel="30" itemValue="30"/>
                                    <apex:selectOption itemLabel="40" itemValue="40"/>
                                    <apex:selectOption itemLabel="50" itemValue="50"/>
                                    <apex:selectOption itemLabel="60" itemValue="60"/>
                                </apex:selectList>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                <apex:commandButton action="{!pc1Frst}" disabled="{!NOT(pc1.hasPrevious)}"
                                                    rerender="pclist" status="pclistPC"
                                                    title="First Page" value="<<"/>&nbsp;&nbsp;
                                <apex:commandButton action="{!pc1Prev}" disabled="{!NOT(pc1.hasPrevious)}"
                                                    rerender="pclist" status="pclistPC"
                                                    title="Prev Page" value="<"/>

                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                                <apex:commandButton action="{!pc1Next}" disabled="{!NOT(pc1.hasNext)}"
                                                    rerender="pclist" status="pclistPC"
                                                    title="Next Page" value=">"/>&nbsp;&nbsp;
                                <apex:commandButton action="{!pc1Last}" disabled="{!NOT(pc1.hasNext)}"
                                                    rerender="pclist" status="pclistPC"
                                                    title="Last Page" value=">>"/>
                                </apex:outputPanel>
                            </apex:facet>
                        </apex:actionStatus>
                    </td>

                    <td class="center" width="15%">
                        <apex:outputText value="Page: {!pc1.pageNumber} of {!pc1.lastPageNumber}"/>
                    </td>
                    </tr>
                    </table>
                </apex:outputPanel>
            </apex:facet>
        </apex:pageBlock>

        <apex:pageBlock rendered="{!pageBlockNumber = 3}">
            <table width="100%"><tr><td align="center">
                <apex:outputPanel >
                    <apex:commandButton action="{!gotoPageBlock1}"
                                        value=" « Back to Search "/>&nbsp;
                </apex:outputPanel>
            </td></tr></table><br/>
        </apex:pageBlock>

    </apex:form>
    <style>
